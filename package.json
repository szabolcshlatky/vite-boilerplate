{
  "name": "vite-boilerplate",
  "description": "Modern vanilla TypeScript boilerplate with Vite.",
  "keywords": [
    "Vite",
    "HTML",
    "SCSS",
    "PostCSS",
    "TailwindCSS",
    "TS",
    "ESNext",
    "ESLint",
    "Prettier",
    "Jest",
    "Husky"
  ],
  "author": "Coding Sable",
  "license": "ISC",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "vite-boilerplate"
  },
  "type": "module",
  "main": "main.ts",
  "engines": {
    "vscode": "^1.22.0",
    "pnpm": ">= 8.10.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint && pnpm run jest"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "jest": "jest"
  },
  "dependencies": {
    "link": "2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/user-event": "14.5.2",
    "@types/eslint": "^8.56.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "autoprefixer": "10.4.16",
    "cssnano": "6.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lodash": "4.17.21",
    "postcss": "8.4.33",
    "prettier": "3.2.2",
    "sass": "1.69.7",
    "tailwindcss": "3.4.1",
    "typescript": "^5.2.2",
    "uuid": "9.0.1",
    "vite": "^5.0.8"
  }
}
